{
    "homepage": "https://github.com/GitTools/GitVersion",
    "description": "Easy Semantic Versioning for projects using Git.",
    "license": "MIT",
    "version": "5.0.2-beta1-60",
    "url": "https://packages.chocolatey.org/GitVersion.Portable.5.0.2-beta1-60.nupkg",
