{
    "homepage": "https://github.com/GitTools/GitVersion",
    "description": "Easy Semantic Versioning for projects using Git.",
    "license": "MIT",
    "version": "5.0.0-beta4-9",
    "url": "https://packages.chocolatey.org/GitVersion.Portable.5.0.0-beta4-9.nupkg",
    "hash": "3cd9ead412bc5c06ad27145c17d8366221cdbe4899a783815cb5902f0da22e3d",
    "extract_dir": "tools",
    "bin": "GitVersion.exe",
    "checkver": {
        "url": "https://chocolatey.org/packages/GitVersion.Portable",
        "regex": "GitVersion ([\\w.-]+)"
