{
    "homepage": "https://github.com/GitTools/GitVersion",
    "description": "Easy Semantic Versioning for projects using Git.",
    "license": "MIT",
    "version": "4.0.1-beta1-65",
    "url": "https://packages.chocolatey.org/GitVersion.Portable.4.0.1-beta1-65.nupkg",
    "hash": "8193d7785ff42037b029b7f8e3aa269a259d2d16444100eec8828f5286c098cf",
    "extract_dir": "tools",
    "bin": "GitVersion.exe",
    "checkver": {
        "url": "https://chocolatey.org/packages/GitVersion.Portable",
        "regex": "GitVersion ([\\w.-]+)"
