{
    "homepage": "https://github.com/GitTools/GitVersion",
    "description": "Easy Semantic Versioning for projects using Git.",
    "license": "MIT",
    "version": "5.0.0-beta1-78",
    "url": "https://packages.chocolatey.org/GitVersion.Portable.5.0.0-beta1-78.nupkg",
    "hash": "fddcb16c5bb635aff115bb9e487d626bbe440112f545ae96f56990c4ce340b23",
    "extract_dir": "tools",
    "bin": "GitVersion.exe",
    "checkver": {
        "url": "https://chocolatey.org/packages/GitVersion.Portable",
        "regex": "GitVersion ([\\w.-]+)"
