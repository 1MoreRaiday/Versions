{
    "homepage": "https://github.com/GitTools/GitVersion",
    "description": "Easy Semantic Versioning for projects using Git.",
    "license": "MIT",
    "version": "5.0.2-beta1-72",
    "url": "https://packages.chocolatey.org/GitVersion.Portable.5.0.2-beta1-72.nupkg",
    "hash": "6ed911d823e55248f0f37c7ad0beae42694d7f834e6e962671352e2bea5e5a07",
    "extract_dir": "tools",
    "bin": "GitVersion.exe",
    "checkver": {
        "url": "https://chocolatey.org/packages/GitVersion.Portable",
        "regex": "GitVersion ([\\w.-]+)"
