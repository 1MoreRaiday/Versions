{
    "homepage": "https://tomcat.apache.org/",
    "version": "7.0.84",
    "architecture": {
        "64bit": {
            "url": "https://www.apache.org/dyn/closer.cgi?action=download&filename=tomcat/tomcat-7/v7.0.84/bin/apache-tomcat-7.0.84-windows-x64.zip",
            "hash": "sha1:006cbba46e4db731dcf6706cf5574652a1d8ae89"
        },
        "32bit": {
            "url": "https://www.apache.org/dyn/closer.cgi?action=download&filename=tomcat/tomcat-7/v7.0.84/bin/apache-tomcat-7.0.84-windows-x86.zip",
            "hash": "sha1:b091de459dc4d1a616a33a621b904db40129bf09"
        }
    },
    "extract_dir": "apache-tomcat-7.0.84",
    "bin": "bin\\catalina.bat",
    "env_set": {
        "CATALINA_HOME": "$dir",
        "CATALINA_BASE": "$dir"
    },
    "suggest": {
        "JRE": [
            "extras/oraclejre-server",
            "openjdk"
        ]
    },
    "checkver": {
        "url": "https://www.apache.org/dist/tomcat/tomcat-7/",
        "re": "v(?<version>7.0.[\\d]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://www.apache.org/dyn/closer.cgi?action=download&filename=tomcat/tomcat-7/v$version/bin/apache-tomcat-$version-windows-x64.zip",
                "hash": {
                    "url": "https://www.apache.org/dist/tomcat/tomcat-7/v$version/bin/apache-tomcat-$version-windows-x64.zip.sha1",
                    "find": "([a-fA-F0-9]+)"
                }
            },
            "32bit": {
                "url": "https://www.apache.org/dyn/closer.cgi?action=download&filename=tomcat/tomcat-7/v$version/bin/apache-tomcat-$version-windows-x86.zip",
                "hash": {
                    "url": "https://www.apache.org/dist/tomcat/tomcat-7/v$version/bin/apache-tomcat-$version-windows-x86.zip.sha1",
                    "find": "([a-fA-F0-9]+)"
                }
            }
        },
        "extract_dir": "apache-tomcat-$version"
    }
}
